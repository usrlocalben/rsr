[
	{"$truecolor": { "id": "root",
		"sRGB": true,
		"program": "Default",
		"gpu": {"$gpu": {
			"targetSize": "globals:windowSize",
			"layers": ["nfoo"]
		}}}},

	{"$layer": { "id": "nfoo",
		"color": {"$float": {"x": 0.333}},
		"camera": "uiCamera",
		"gl": [ "nField" ]}},

	{"$translate": {"id":"nField",
		"gl": {"$fxMC": {
			"material": "mBlack",
			"frob": "globals:wallclock",
			"precision": 32,
			"range": 3.2,
			"forkDepth": 3}},
		"scale": {"$float": {"x": 1.0}},
		"rotate": "fieldrotator"}},

	{"$material": {"id": "mWire", "program": "Wireframe"}},

	{"$material": { "id": "mBlack",
		"program": "Envmap",
		"filter": false,
		"texture0": {"$image": {"file": "data/textures/env-black.png"}}}},

	{"$vec3": {"id": "norotator", "x": 0, "y": 0, "z": 0}},

	{"$computedVec3": { "id": "fieldrotator",
		"inputs": [{"name": "t", "type": "real", "source": "globals:wallclock"}],
		"code": "{t*0.1, sin(t*0.41)*0.333, sin(t*0.222)*0.3}"}}
]
