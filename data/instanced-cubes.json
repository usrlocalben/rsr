[
	{"$glow": { "id": "root",
		"sRGB": true,
		"image": "main:color",
		"blur": {"$kawase": {
			"intensity": 2,
			"taskSize": 16,
			"input": "main:half"}}}},

	{"$buffers": { "id": "main",
		"color": true,
		"half": true,
		"gpu": {"$gpu": {
			"targetSize": "globals:windowSize",
			"layers": ["mainLayer"]}}}},

	{"$layer": { "id": "mainLayer",
		"color": {"$computedVec3": {
			"code": "{0.222, 0.222, 0.333}"}},
		"camera": {"$perspective": {
			"position": {"$vec3": {"x": -18.5, "y": 8.5, "z": 44.0}},
			"h": 3.0, "v": -0.4,
			"fov": 45.0,
			"originX": 0}},
		"gl": [ "lotsOfCubes" ]}},

	{"$computedVec3": { "id": "lotsRotation",
		"inputs": [{"name": "t", "type": "real", "source": "globals:wallclock"}],
		"code": "{pow(sin(t*0.25), 100)*0.05, t*0.005, 0}"}},

	{"$translate": {"id": "lotsOfCubes",
		"rotate": "lotsRotation",
		"gl": {"$fxMany": {
			"material": {"$material": {"program": "Many"}},
			"mesh": "mycube.obj"}}}}
]
